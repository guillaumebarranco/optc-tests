    {
      "regex": "^@support",
      "orderLevel": 130,
      "numberOfEmptyLinesAfterGroup": 0
    },
    {
      "regex": "^@vitals-plus",
      "orderLevel": 160,
      "numberOfEmptyLinesAfterGroup": 0
    },
    {
      "regex": "^@vitals-stream",
      "orderLevel": 170,
      "numberOfEmptyLinesAfterGroup": 0
    },
    {
      "regex": "^[.]",
      "orderLevel": 200
    }
  ],
  "importSorter.generalConfiguration.exclude": ["polyfills.ts"],
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "search.usePCRE2": true
}
